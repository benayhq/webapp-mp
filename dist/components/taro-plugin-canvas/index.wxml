<block wx:if="{{$taroCompReady}}">
    <block wx:if="{{pxWidth && pxHeight}}">
        <canvas canvas-id="{{canvasId}}" style="{{'width:' + pxWidth * 2 + 'rpx; height:' + pxHeight * 2 + 'rpx;'}}" class="{{(debug ? 'debug' : 'pro') + ' canvas'}}"></canvas>
    </block>
    <block wx:else="{{true}}">
        <view></view>
    </block>
</block>