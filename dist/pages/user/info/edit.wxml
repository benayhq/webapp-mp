<block>
    <view class="mp-edit-user">
        <at-message __triggerObserer="{{ _triggerObserer }}"></at-message>
        <at-form __triggerObserer="{{ _triggerObserer }}">
            <at-input name="value1" __triggerObserer="{{ _triggerObserer }}" title="姓名"
            type="text" placeholder="(选填)" value="{{userName}}" bindonchange="handleUserNameChange"
            __fn_onChange="{{true}}" data-e-handleUserNameChange-so="this"></at-input>
            <at-input name="value6" __triggerObserer="{{ _triggerObserer }}"
            title="手机号码" type="phone" placeholder="请输入常用手机号" value="{{cellPhone}}"
            bindonchange="handleMobileChange" __fn_onChange="{{true}}" data-e-handleMobileChange-so="this"></at-input>
            <at-input name="value1" __triggerObserer="{{ _triggerObserer }}"
            title="微信" type="text" placeholder="客户通过微信与您联系" value="{{weixin}}" bindonchange="handleWeChatChange"
            __fn_onChange="{{true}}" data-e-handleWeChatChange-so="this"></at-input>
            <view class="address">
                <text class="activelabel">服务地址</text>
                <text class="time"></text>
                <view class="region">
                    <region region="{{region}}" __triggerObserer="{{ _triggerObserer }}" bindongetregion="onGetRegion"
                    __fn_onGetRegion="{{true}}" data-e-onGetRegion-so="this"></region>
                </view>
            </view>
            <view class="qrCode">
                <text class="label">微信二维码</text>
            </view>
            <at-image-picker showAddBtn="{{files.length >= 1 ? false : true}}" __triggerObserer="{{ _triggerObserer }}"
            className="uploadPicker" files="{{files}}" bindonchange="handleUploadChange"
            __fn_onChange="{{true}}" data-e-handleUploadChange-so="this"></at-image-picker>
        </at-form>
        <view class="user-submit">
            <at-button type="primary" __triggerObserer="{{ _triggerObserer }}" bindonclick="handleSaveUserInfo"
            __fn_onClick="{{true}}" data-e-handleSaveUserInfo-so="this">保存</at-button>
        </view>
    </view>
</block>