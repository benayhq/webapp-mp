<block wx:if="{{$taroCompReady}}">
    <view>
        <block>
            <block wx:if="{{newFilterList.length === 0}}">
                <empty></empty>
            </block>
            <block wx:else>
                <block wx:if="{{newFilterList}}">
                    <view class="mp-user__product" bindtap="handleProductEdit" wx:for="{{loopArray318}}" wx:for-item="item" wx:for-index="_anonIdx2" data-e-tap-so="this" data-e-tap-a-a="{{item.$original.value}}">
                        <view>
                            <image className="icon-header" src="{{item.$original.imgUrl}}"></image>
                        </view>
                        <view>
                            <view class="mp-user__product-desc">{{item.$original.desc}}</view>
                            <view class="mp-user__product-price">￥{{item.$original.price}} <text class="mp-user__product-marketprice">￥{{item.$original.marketPrice}}</text>
                            </view>
                            <view class="mp-user__product-prePrice">预定金: {{item.$original.prePrice}}</view>
                        </view>
                        <view class="item-extra__icon item-extra__icon">
                            <view class="product-edit">编 辑</view><text class="at-icon at-icon item-extra__icon-arrow item-extra__icon-arrow at-icon-chevron-right at-icon-chevron-right product-appointer"></text>
                        </view>
                    </view>
                </block>
            </block>
        </block>
    </view>
</block>