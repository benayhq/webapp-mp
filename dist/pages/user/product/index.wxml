<block>
    <view>
        <block>
            <block wx:if="{{newFilterList.length === 0}}">
                <empty __triggerObserer="{{ _triggerObserer }}"></empty>
            </block>
            <block wx:if="{{newFilterList}}" wx:else>
                <view class="mp-user__product" bindtap="handleProductEdit" wx:for="{{newFilterList}}"
                wx:for-item="item" data-e-handleProductEdit-so="this" data-e-handleProductEdit-a-a="{{item.value}}">
                    <view>
                        <image className="icon-header" __triggerObserer="{{ _triggerObserer }}"
                        src="{{item.imgUrl}}" />
                    </view>
                    <view>
                        <view class="mp-user__product-desc">{{item.desc}}</view>
                        <view class="mp-user__product-price">￥{{item.price}}
                            <text class="mp-user__product-marketprice">￥{{item.marketPrice}}</text>
                        </view>
                        <view class="mp-user__product-prePrice">预定金: {{item.prePrice}}</view>
                    </view>
                    <view class="item-extra__icon item-extra__icon" __triggerObserer="{{ _triggerObserer }}">
                        <view class="product-edit">编 辑</view>
                        <text class="at-icon at-icon item-extra__icon-arrow item-extra__icon-arrow at-icon-chevron-right at-icon-chevron-right product-appointer"
                        __triggerObserer="{{ _triggerObserer }}"></text>
                    </view>
                    <view __triggerObserer="{{ _triggerObserer }}">往期产品</view>
                </view>
            </block>
        </block>
    </view>
</block>