<block>
    <view class="mp-history">
        <block wx:if="{{activeList}}">
            <view class="list-wrapper" wx:for="{{activeList}}" wx:for-item="item" wx:for-index="_anonIdx">
                <view>
                    <image className="icon-header" __triggerObserer="{{ _triggerObserer }}" src="{{item.url}}"></image>
                </view>
                <view>
                    <view class="mp-active-group">{{item.name}}</view>
                    <view class="mp-active-group">{{item.people}}人成团</view>
                </view>
            </view>
        </block>
    </view>
</block>