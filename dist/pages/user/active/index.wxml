<block wx:if="{{$taroCompReady}}">
    <view>
        <scroll-view scroll-y="{{true}}" bindscrolltolower="loadMore" style="{{anonymousState__temp3}}">
            <block>
                <block wx:if="{{!pageLoaded}}">
                    <loading></loading>
                </block>
                <block wx:elif="{{activeList.length > 0}}">
                    <at-list>
                        <block wx:if="{{activeList}}">
                            <at-list-item wx:for="{{loopArray319}}" wx:for-item="item" wx:for-index="_anonIdx2" compid="{{item.$compid__1152}}"></at-list-item>
                        </block>
                    </at-list>
                </block>
                <block wx:else>
                    <view>
                        <empty></empty>
                    </view>
                </block>
            </block>
            <block wx:if="{{pageLoaded && loading}}">
                <view class="home__loading"><text class="home__loading-txt">正在加载中...</text>
                </view>
            </block>
            <block wx:if="{{!hasMore}}">
                <view class="home__loading home__loading--not-more"><text class="home__loading-txt">没有更多了</text>
                </view>
            </block>
        </scroll-view>
    </view>
</block>