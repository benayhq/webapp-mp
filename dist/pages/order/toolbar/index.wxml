<block wx:if="{{$taroCompReady}}">
    <block wx:if="{{toolBar == null || toolBar == undefined}}">
        <view></view>
    </block>
    <block wx:elif="{{toolBar && toolBar[0] && toolBar[1]}}">
        <view class="toolbar">
            <at-button compid="{{$compid__69}}">{{toolBar[0].text}}</at-button> <text class="margin40"></text>
            <at-button compid="{{$compid__70}}">{{toolBar[1].text}}</at-button> <text class="margin40"></text>
            <modal compid="{{$compid__71}}"></modal>
        </view>
    </block>
    <block wx:elif="{{toolBar && toolBar[0]}}">
        <view class="toolbar">
            <block wx:if="{{toolBar}}">
                <at-button compid="{{$compid__72}}">{{toolBar[0].text}}</at-button>
            </block> <text class="margin40"></text>
        </view>
    </block>
    <block wx:elif="{{toolBar && toolBar[1]}}">
        <view class="toolbar">
            <block wx:if="{{toolBar}}">
                <at-button compid="{{$compid__73}}">{{toolBar[1].text}}</at-button>
            </block><text class="margin40"></text>
        </view>
    </block>
</block>